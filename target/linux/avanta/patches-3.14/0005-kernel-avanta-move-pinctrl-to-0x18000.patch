From c931cdb5ed4a9c085f6438fbc64bb3fa03f63759 Mon Sep 17 00:00:00 2001
From: Claudio Leite <leitec@staticky.com>
Date: Thu, 31 Jul 2014 21:29:46 -0400
Subject: [PATCH 1/3] kernel: avanta: move pinctrl to 0x18000

---
 arch/arm/boot/dts/avanta-6560.dtsi | 4 ++--
 arch/arm/boot/dts/avanta.dtsi      | 4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/avanta-6560.dtsi b/arch/arm/boot/dts/avanta-6560.dtsi
index e491567..0b9911c 100644
--- a/arch/arm/boot/dts/avanta-6560.dtsi
+++ b/arch/arm/boot/dts/avanta-6560.dtsi
@@ -55,9 +55,9 @@
 		};
 	};
 	ocp@f1000000 {
-		pinctrl: pinctrl@10000 {
+		pinctrl: pinctrl@18000 {
 			compatible = "marvell,88f6282-pinctrl";
-			reg = <0x10000 0x20>;
+			reg = <0x18000 0x20>;
 
 			pmx_nand: pmx-nand {
 				marvell,pins = "mpp0", "mpp1", "mpp2", "mpp3",
diff --git a/arch/arm/boot/dts/avanta.dtsi b/arch/arm/boot/dts/avanta.dtsi
index 4339462..7cbce7e 100644
--- a/arch/arm/boot/dts/avanta.dtsi
+++ b/arch/arm/boot/dts/avanta.dtsi
@@ -87,11 +87,11 @@
 			status = "disabled";
 		};
 
-		gpio0: gpio@10100 {
+		gpio0: gpio@18100 {
 			compatible = "marvell,orion-gpio";
 			#gpio-cells = <2>;
 			gpio-controller;
-			reg = <0x10100 0x40>;
+			reg = <0x18100 0x40>;
 			ngpios = <32>;
 			interrupt-controller;
 			#interrupt-cells = <2>;
-- 
2.0.3

